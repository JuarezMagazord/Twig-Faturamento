{% set aliquotaIPI = 0.00 %}

    {% if self.configuracaoParametros %}
        {% if self.origemFiscal in (2) %}
            {% set aliquotaIPI = 0.00 %}
        {% elseif self.aliquotaProdutoDerivacao|length > 0 and self.aliquotaProdutoDerivacao[self.notaFiscalItem.produtoDerivacao.codigo]  is defined %}    
            {% set aliquotaIPI = (self.aliquotaProdutoDerivacao[self.notaFiscalItem.produtoDerivacao.codigo]) %}
        {% elseif self.aliquotaNcm|length > 0 and self.aliquotaNcm[self.notaFiscalItem.ncm.codigo]  is defined %}
            {% set aliquotaIPI = (self.aliquotaNcm[self.notaFiscalItem.ncm.codigo]) %}
        {% elseif self.aliquotaCfops|length > 0 and self.aliquotaCfops[self.notaFiscalItem.cfop.codigo]  is defined %}
            {% set aliquotaIPI = (self.aliquotaCfops[self.notaFiscalItem.cfop.codigo]) %}    
        {% else %}
            {% set aliquotaIPI = 0.00 %}
        {% endif %}    
    {% else %}
        {% set aliquotaIPI = 0.00 %}
    {% endif %}
    
    {{ self.result(aliquotaIPI) }}